  <HediffDef Name="WVC_HediffMutation_Ability" ParentName="WVC_HediffMutation_Base" Abstract="True">
	<!-- <defaultLabelColor>(190,162,255)</defaultLabelColor> -->
	<defaultLabelColor>(255,162,100)</defaultLabelColor>
	<label>psy-neurons</label>
	<description>As a result of the mutation, neural connections were formed that are sensitive to the psy-field, due to which psychic sensitivity was increased and a new psycast was studied.</description>
	<stages>
	  <li>
		<!-- <becomeVisible>false</becomeVisible> -->
		<partEfficiencyOffset>0.05</partEfficiencyOffset>
		<statOffsets>
		  <PsychicSensitivity>0.10</PsychicSensitivity>
		</statOffsets>
	  </li>
	</stages>
	<comps>
	</comps>
  </HediffDef>

