					<li>Alien_DryadMother_WVC</li>
					<li>ludeon.rimworld</li>
					<li>4</li>
					<li>false</li>
				</arguments>
			</Operation>

			<!-- ===================================================== -->

			<Operation Class="XmlExtensions.ApplyPatch">
				<patchName>WVC_SkinSelectorPatch_ImageVersion</patchName>
				<arguments>
					<li>Marker_HG_Valkyrie_BodieType_WVC</li>
