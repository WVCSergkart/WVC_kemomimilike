  <ScenarioDef ParentName="Marker_ScenarioValkyrie_WVC" Name="Base_ScenarioIncubator_WVC" Abstract="True">
	<label>&lt;color=#a2adff&gt;Incubated: CAT&lt;/color&gt;</label>
	<description>The tasks of a cat are simple from birth to the very end of the cycle - eat, sleep, eat again. But your task is to serve the cats and the whole complex for their incubation... or just leave the job to a more responsible human.</description>
	<scenario>
	  <summary>A medium difficulty scenario with an cat incubator.</summary>
	  <parts>

			<li Class="ScenPart_ConfigPage_ConfigureStartingPawns">
				<def>ConfigPage_ConfigureStartingPawns</def>
				<pawnCount>3</pawnCount>
				<pawnChoiceCount>7</pawnChoiceCount>
			</li>
			<li Class="ScenPart_PlayerPawnsArriveMethod">
				<def>PlayerPawnsArriveMethod</def>
			</li>

			<!--=====================================================================-->

			<li Class="ScenPart_StatFactor">
				<def>StatFactor</def>
				<stat>MinimumHandlingSkill</stat>
			</li>
			<li Class="ScenPart_StatFactor">
				<def>StatFactor</def>
				<stat>SellPriceFactor</stat>
				<factor>0.7</factor>
			</li>

			<!--=====================================================================-->

			<!-- <li Class="ScenPart_StatFactor"> -->
				<!-- <def>StatFactor</def> -->
				<!-- <stat>WorkSpeedGlobal</stat> -->
				<!-- <factor>1.2</factor> -->
			<!-- </li> -->
			<li Class="ScenPart_StatFactor">
				<def>StatFactor</def>
				<stat>WorkToMake</stat>
				<factor>1.2</factor>
			</li>
			<li Class="ScenPart_StatFactor">
				<def>StatFactor</def>
				<stat>WorkToBuild</stat>
				<factor>1.5</factor>
			</li>
			<li Class="ScenPart_StatFactor">
				<def>StatFactor</def>
				<stat>MarketValue</stat>
				<factor>1.2</factor>
			</li>

			<!--=====================================================================-->

			<li Class="ScenPart_StatFactor">
				<def>StatFactor</def>
				<stat>WorkTableEfficiencyFactor</stat>
				<factor>0.8</factor>
			</li>
			<li Class="ScenPart_StatFactor">
				<def>StatFactor</def>
				<stat>AnimalGatherYield</stat>
				<factor>0.5</factor>
			</li>
			<li Class="ScenPart_StatFactor">
				<def>StatFactor</def>
				<stat>PlantHarvestYield</stat>
				<factor>0.6</factor>
			</li>
			<li Class="ScenPart_StatFactor">
				<def>StatFactor</def>
				<stat>MeatAmount</stat>
				<factor>0.5</factor>
			</li>
			<li Class="ScenPart_StatFactor">
				<def>StatFactor</def>
				<stat>LeatherAmount</stat>
				<factor>0.7</factor>
			</li>
			<li Class="ScenPart_StatFactor">
				<def>StatFactor</def>
				<stat>MiningYield</stat>
				<factor>0.5</factor>
			</li>
			<li Class="ScenPart_StatFactor">
				<def>StatFactor</def>
				<stat>ButcheryFleshEfficiency</stat>
				<factor>0.7</factor>
			</li>
			<li Class="ScenPart_StatFactor">
				<def>StatFactor</def>
				<stat>ButcheryMechanoidEfficiency</stat>
				<factor>0.7</factor>
			</li>

			<!--=====================================================================-->

			<li Class="ScenPart_StatFactor">
				<def>StatFactor</def>
				<stat>Mass</stat>
				<factor>0.8</factor>
			</li>

			<!--=====================================================================-->

			<li Class="ScenPart_StatFactor">
				<def>StatFactor</def>
				<stat>EatingSpeed</stat>
				<factor>2</factor>
			</li>
			<li Class="ScenPart_StatFactor">
				<def>StatFactor</def>
				<stat>JoyGainFactor</stat>
