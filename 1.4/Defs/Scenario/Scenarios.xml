				<def>StatFactor</def>
				<stat>CaravanRidingSpeedFactor</stat>
				<factor>2</factor>
			</li>
			<li Class="ScenPart_StatFactor">
				<def>StatFactor</def>
				<stat>Nutrition</stat>
				<factor>2.0</factor>
			</li>

			<!--=====================================================================-->

			<!-- <li MayRequire="honkai.girls.sergkart" Class="ScenPart_DisableIncident"> -->
				<!-- <def>DisableIncident</def> -->
				<!-- <incident>WandererJoin_Valkyrie_WVC</incident> -->
