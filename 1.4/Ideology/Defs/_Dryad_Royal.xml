		</surpriseAttack>
	  </li>
	  <li>
		<label>feet</label>
		<capacities>
		  <li>Blunt</li>
		  <!-- <li>Stab</li> -->
		</capacities>
		<power>13</power>
		<cooldownTime>1.5</cooldownTime>
		<linkedBodyPartsGroup>Feet</linkedBodyPartsGroup>
		<chanceFactor>0.5</chanceFactor>
		<armorPenetration>0.1</armorPenetration>
		<surpriseAttack>
		  <extraMeleeDamages>
			<li>
			  <def>Stun</def>
			  <amount>7</amount>
			  <chance>1.0</chance>
			</li>
		  </extraMeleeDamages>
		</surpriseAttack>
	  </li>
